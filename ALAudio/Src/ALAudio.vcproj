<?xml version="1.0" encoding = "Windows-1252"?>
<VisualStudioProject
	ProjectType="Visual C++"
	Version="7.00"
	Name="ALAudio"
	SccProjectName="Perforce Project"
	SccAuxPath=""
	SccLocalPath="."
	SccProvider="MSSCCI:Perforce SCM">
	<Platforms>
		<Platform
			Name="Win32"/>
	</Platforms>
	<Configurations>
		<Configuration
			Name="Release|Win32"
			OutputDirectory=".\..\Lib"
			IntermediateDirectory=".\Release"
			ConfigurationType="2"
			UseOfMFC="0"
			ATLMinimizesCRunTimeLibraryUsage="FALSE"
			CharacterSet="1">
			<Tool
				Name="VCCLCompilerTool"
				Optimization="3"
				GlobalOptimizations="TRUE"
				InlineFunctionExpansion="2"
				EnableIntrinsicFunctions="TRUE"
				FavorSizeOrSpeed="1"
				OmitFramePointers="FALSE"
				OptimizeForProcessor="2"
				AdditionalIncludeDirectories="..\..\Core\Inc,..\..\Engine\Inc,..\..\OpenAL\Inc,..\Inc,..\..\Vorbis\Inc,..\..\metoolkit\include"
				PreprocessorDefinitions="NDEBUG;_MBCS;_USRDLL;ALAUDIO_EXPORTS;_WINDOWS;UNICODE;_WIN32_IE=0x0200;WIN32;ALAUDIO_API=DLL_EXPORT"
				MinimalRebuild="FALSE"
				RuntimeLibrary="2"
				StructMemberAlignment="3"
				BufferSecurityCheck="FALSE"
				EnableFunctionLevelLinking="TRUE"
				ForceConformanceInForLoopScope="TRUE"
				UsePrecompiledHeader="2"
				PrecompiledHeaderThrough="ALAudio.h"
				PrecompiledHeaderFile=".\Release/ALAudio.pch"
				AssemblerOutput="4"
				AssemblerListingLocation=".\Release/"
				ObjectFile=".\Release/"
				ProgramDataBaseFileName=".\Release/"
				BrowseInformation="0"
				WarningLevel="4"
				SuppressStartupBanner="TRUE"
				DebugInformationFormat="0"
				CompileAs="0"/>
			<Tool
				Name="VCCustomBuildTool"
				CommandLine="xcopy /Y $(IntDir)\ALAudio.dll ..\..\System
"
				Outputs="..\..\System\ALAudio.dll"/>
			<Tool
				Name="VCLinkerTool"
				AdditionalOptions="/MACHINE:I386"
				AdditionalDependencies="..\..\Core\Lib\Core.lib ..\..\Engine\Lib\Engine.lib odbc32.lib odbccp32.lib ..\..\Vorbis\Lib\ogg.lib ..\..\Vorbis\Lib\vorbis.lib ..\..\Vorbis\Lib\vorbisfile.lib"
				OutputFile="$(IntDir)\ALAudio.dll"
				LinkIncremental="2"
				SuppressStartupBanner="TRUE"
				IgnoreDefaultLibraryNames="LIBCD;LIBCMTD;MSVCRTD;LIBC;LIBCMT"
				ProgramDatabaseFile=".\Release/ALAudio.pdb"
				SubSystem="2"
				OptimizeReferences="0"
				ImportLibrary=".\Release/ALAudio.lib"/>
			<Tool
				Name="VCMIDLTool"
				PreprocessorDefinitions="NDEBUG"
				MkTypLibCompatible="TRUE"
				SuppressStartupBanner="TRUE"
				TargetEnvironment="1"
				TypeLibraryName=".\Release/ALAudio.tlb"/>
			<Tool
				Name="VCPostBuildEventTool"/>
			<Tool
				Name="VCPreBuildEventTool"/>
			<Tool
				Name="VCPreLinkEventTool"/>
			<Tool
				Name="VCResourceCompilerTool"
				PreprocessorDefinitions="NDEBUG"
				Culture="1033"/>
			<Tool
				Name="VCWebServiceProxyGeneratorTool"/>
			<Tool
				Name="VCWebDeploymentTool"/>
		</Configuration>
		<Configuration
			Name="Debug|Win32"
			OutputDirectory=".\..\Lib"
			IntermediateDirectory=".\Debug"
			ConfigurationType="2"
			UseOfMFC="0"
			ATLMinimizesCRunTimeLibraryUsage="FALSE"
			CharacterSet="1">
			<Tool
				Name="VCCLCompilerTool"
				Optimization="0"
				AdditionalIncludeDirectories="..\..\Core\Inc,..\..\Engine\Inc,..\..\OpenAL\Inc,..\Inc,..\..\Vorbis\Inc,..\..\metoolkit\include"
				PreprocessorDefinitions="_DEBUG;_MBCS;_USRDLL;ALAUDIO_EXPORTS;_WINDOWS;UNICODE;_WIN32_IE=0x0200;WIN32;ALAUDIO_API=DLL_EXPORT"
				MinimalRebuild="TRUE"
				BasicRuntimeChecks="2"
				RuntimeLibrary="2"
				StructMemberAlignment="3"
				ForceConformanceInForLoopScope="TRUE"
				UsePrecompiledHeader="2"
				PrecompiledHeaderThrough="ALAudio.h"
				PrecompiledHeaderFile=".\Debug/ALAudio.pch"
				AssemblerOutput="4"
				AssemblerListingLocation=".\Debug/"
				ObjectFile=".\Debug/"
				ProgramDataBaseFileName=".\Debug/"
				BrowseInformation="1"
				WarningLevel="4"
				SuppressStartupBanner="TRUE"
				DebugInformationFormat="4"
				CompileAs="0"/>
			<Tool
				Name="VCCustomBuildTool"
				CommandLine="xcopy /Y $(IntDir)\ALAudio.dll ..\..\System
"
				Outputs="..\..\System\ALAudio.dll"/>
			<Tool
				Name="VCLinkerTool"
				AdditionalOptions="/MACHINE:I386"
				AdditionalDependencies="..\..\Core\Lib\Core.lib ..\..\Engine\Lib\Engine.lib odbc32.lib odbccp32.lib  ..\..\Vorbis\Lib\ogg.lib ..\..\Vorbis\Lib\vorbis.lib ..\..\Vorbis\Lib\vorbisfile.lib"
				OutputFile="$(IntDir)\ALAudio.dll"
				LinkIncremental="2"
				SuppressStartupBanner="TRUE"
				IgnoreDefaultLibraryNames="LIBCD;LIBCMTD;MSVCRTD;LIBC;LIBCMT"
				GenerateDebugInformation="TRUE"
				ProgramDatabaseFile=".\Debug/ALAudio.pdb"
				SubSystem="2"
				ImportLibrary=".\Debug/ALAudio.lib"/>
			<Tool
				Name="VCMIDLTool"
				PreprocessorDefinitions="_DEBUG"
				MkTypLibCompatible="TRUE"
				SuppressStartupBanner="TRUE"
				TargetEnvironment="1"
				TypeLibraryName=".\Debug/ALAudio.tlb"/>
			<Tool
				Name="VCPostBuildEventTool"/>
			<Tool
				Name="VCPreBuildEventTool"/>
			<Tool
				Name="VCPreLinkEventTool"/>
			<Tool
				Name="VCResourceCompilerTool"
				PreprocessorDefinitions="_DEBUG"
				Culture="1033"/>
			<Tool
				Name="VCWebServiceProxyGeneratorTool"/>
			<Tool
				Name="VCWebDeploymentTool"/>
		</Configuration>
	</Configurations>
	<Files>
		<Filter
			Name="Inc"
			Filter="">
			<File
				RelativePath="..\Inc\ALAudio.h">
			</File>
			<File
				RelativePath="..\Inc\ALAudioSubsystem.h">
			</File>
		</Filter>
		<Filter
			Name="Src"
			Filter="">
			<File
				RelativePath=".\ALAudio.cpp">
			</File>
			<File
				RelativePath="ALAudioEAXTools.cpp">
			</File>
			<File
				RelativePath=".\ALAudioPrivate.h">
			</File>
			<File
				RelativePath=".\ALAudioSubsystem.cpp">
			</File>
		</Filter>
	</Files>
	<Globals>
	</Globals>
</VisualStudioProject>
