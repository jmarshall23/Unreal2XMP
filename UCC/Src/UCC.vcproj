<?xml version="1.0" encoding = "Windows-1252"?>
<VisualStudioProject
	ProjectType="Visual C++"
	Version="7.00"
	Name="UCC"
	SccProjectName="Perforce Project"
	SccAuxPath=""
	SccLocalPath=".."
	SccProvider="MSSCCI:Perforce SCM">
	<Platforms>
		<Platform
			Name="Win32"/>
	</Platforms>
	<Configurations>
		<Configuration
			Name="Release|Win32"
			OutputDirectory=".\..\Lib"
			IntermediateDirectory=".\Release"
			ConfigurationType="1"
			UseOfMFC="0"
			ATLMinimizesCRunTimeLibraryUsage="FALSE"
			CharacterSet="1">
			<Tool
				Name="VCCLCompilerTool"
				Optimization="3"
				GlobalOptimizations="TRUE"
				InlineFunctionExpansion="2"
				EnableIntrinsicFunctions="TRUE"
				FavorSizeOrSpeed="1"
				OmitFramePointers="FALSE"
				OptimizeForProcessor="2"
				AdditionalIncludeDirectories="..\..\Core\Inc,..\..\Engine\Inc,..\..\Filer\Inc,..\Inc,..\..\metoolkit\include"
				PreprocessorDefinitions="_CONSOLE,NDEBUG,UNICODE,_WIN32_IE=0x0200,WIN32"
				StringPooling="TRUE"
				MinimalRebuild="FALSE"
				RuntimeLibrary="2"
				StructMemberAlignment="3"
				BufferSecurityCheck="FALSE"
				EnableFunctionLevelLinking="TRUE"
				ForceConformanceInForLoopScope="TRUE"
				PrecompiledHeaderFile=".\Release/UCC.pch"
				AssemblerListingLocation=".\Release/"
				ObjectFile=".\Release/"
				ProgramDataBaseFileName=".\Release/"
				BrowseInformation="0"
				WarningLevel="4"
				SuppressStartupBanner="TRUE"
				DebugInformationFormat="0"
				CompileAs="0"/>
			<Tool
				Name="VCCustomBuildTool"
				CommandLine="xcopy /Y $(IntDir)\UCC.exe ..\..\System
"
				Outputs="..\..\System\UCC.exe"/>
			<Tool
				Name="VCLinkerTool"
				AdditionalOptions="/MACHINE:I386"
				AdditionalDependencies="..\..\Core\Lib\Core.lib ..\..\zlib\Release\zlib.lib"
				OutputFile="$(IntDir)\UCC.exe"
				LinkIncremental="2"
				SuppressStartupBanner="TRUE"
				IgnoreDefaultLibraryNames="LIBCD;LIBCMTD;MSVCRTD;LIBC;LIBCMT"
				ProgramDatabaseFile=".\..\Lib/UCC.pdb"
				SubSystem="1"
				OptimizeReferences="0"/>
			<Tool
				Name="VCMIDLTool"
				TypeLibraryName=".\..\Lib/UCC.tlb"/>
			<Tool
				Name="VCPostBuildEventTool"/>
			<Tool
				Name="VCPreBuildEventTool"/>
			<Tool
				Name="VCPreLinkEventTool"/>
			<Tool
				Name="VCResourceCompilerTool"
				PreprocessorDefinitions="NDEBUG"
				Culture="1033"/>
			<Tool
				Name="VCWebServiceProxyGeneratorTool"/>
			<Tool
				Name="VCWebDeploymentTool"/>
		</Configuration>
		<Configuration
			Name="Debug|Win32"
			OutputDirectory=".\..\Lib"
			IntermediateDirectory=".\Debug"
			ConfigurationType="1"
			UseOfMFC="0"
			ATLMinimizesCRunTimeLibraryUsage="FALSE"
			CharacterSet="1">
			<Tool
				Name="VCCLCompilerTool"
				Optimization="0"
				AdditionalIncludeDirectories="..\..\Core\Inc,..\..\Engine\Inc,..\..\Filer\Inc,..\Inc,..\..\metoolkit\include"
				PreprocessorDefinitions="_CONSOLE,UNICODE,_DEBUG,_WIN32_IE=0x0200,WIN32"
				MinimalRebuild="TRUE"
				BasicRuntimeChecks="2"
				RuntimeLibrary="2"
				StructMemberAlignment="3"
				ForceConformanceInForLoopScope="TRUE"
				PrecompiledHeaderFile=".\Debug/UCC.pch"
				AssemblerListingLocation=".\Debug/"
				ObjectFile=".\Debug/"
				ProgramDataBaseFileName=".\Debug/"
				BrowseInformation="1"
				WarningLevel="4"
				SuppressStartupBanner="TRUE"
				DebugInformationFormat="4"
				CompileAs="0"/>
			<Tool
				Name="VCCustomBuildTool"
				CommandLine="xcopy /Y $(IntDir)\UCC.exe ..\..\System
"
				Outputs="..\..\System\UCC.exe"/>
			<Tool
				Name="VCLinkerTool"
				AdditionalOptions="/MACHINE:I386"
				AdditionalDependencies="..\..\Core\Lib\Core.lib  ..\..\zlib\Debug\zlib.lib"
				OutputFile="$(IntDir)\UCC.exe"
				LinkIncremental="2"
				SuppressStartupBanner="TRUE"
				IgnoreDefaultLibraryNames="LIBCD;LIBCMTD;MSVCRTD;LIBC;LIBCMT"
				GenerateDebugInformation="TRUE"
				ProgramDatabaseFile=".\..\Lib/UCC.pdb"
				SubSystem="1"/>
			<Tool
				Name="VCMIDLTool"
				TypeLibraryName=".\..\Lib/UCC.tlb"/>
			<Tool
				Name="VCPostBuildEventTool"/>
			<Tool
				Name="VCPreBuildEventTool"/>
			<Tool
				Name="VCPreLinkEventTool"/>
			<Tool
				Name="VCResourceCompilerTool"
				PreprocessorDefinitions="NDEBUG"
				Culture="1033"/>
			<Tool
				Name="VCWebServiceProxyGeneratorTool"/>
			<Tool
				Name="VCWebDeploymentTool"/>
		</Configuration>
	</Configurations>
	<Files>
		<Filter
			Name="Src"
			Filter="cpp;c;cxx;rc;def;r;odl;idl;hpj;bat">
			<File
				RelativePath=".\UCC.cpp">
			</File>
		</Filter>
	</Files>
	<Globals>
	</Globals>
</VisualStudioProject>
