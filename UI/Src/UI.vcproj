			ConfigurationType="2"
			UseOfMFC="0"
			ATLMinimizesCRunTimeLibraryUsage="FALSE"
			CharacterSet="1">
			<Tool
				Name="VCCLCompilerTool"
				AdditionalOptions="/Zm512 "
				Optimization="3"
				GlobalOptimizations="TRUE"
				InlineFunctionExpansion="2"
				EnableIntrinsicFunctions="TRUE"
				FavorSizeOrSpeed="1"
				OptimizeForProcessor="2"
				AdditionalIncludeDirectories="..\Inc,..\..\Core\Inc,..\..\Engine\Inc,..\..\metoolkit\include,..\..\Core\Loki"
				PreprocessorDefinitions="UI_API=__declspec(dllexport);_WINDOWS;NDEBUG;UNICODE;WIN32"
