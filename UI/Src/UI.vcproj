				Name="VCCustomBuildTool"
				CommandLine="xcopy /Y $(IntDir)\UI.dll ..\..\System
"
				Outputs="..\..\System\UI.dll"/>
			<Tool
				Name="VCLinkerTool"
				AdditionalOptions="/MACHINE:I386"
