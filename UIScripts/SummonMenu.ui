State=NullComponent
State=SummonMenuPage
Transition=HideSummonMenu,1,0,0.5,SummonMenuPage
Transition=ShowSummonMenu,0,1,0.5,SummonMenuPage
NextTransitionEvent=ToggleSummonMenu
TweenColor=true
Alignment=Left,Top
Location=0,150

[NullComponent]
Class=RealComponent
Location=-256,0
Color=0,0,0,0

[Mouse]
Class=MouseCursor
Image=MouseCursor

[SummonClose]
