Component=Label
	Font=Engine.smallfont
	Text=[X]
	MouseClick=Event,HideSummonMenu
Component=KeyEvent
	Key=Escape,Press,Event,HideSummonMenu
	Key=Mouse4,Release,Event,HideSummonMenu
Align=Right,Top
Location=16,0
DrawOrder=1

