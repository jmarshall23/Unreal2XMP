<?xml version="1.0" encoding = "Windows-1252"?>
<VisualStudioProject
	ProjectType="Visual C++"
	Version="7.00"
	Name="UWeb"
	SccProjectName="Perforce Project"
	SccAuxPath=""
	SccLocalPath=".."
	SccProvider="MSSCCI:Perforce SCM">
	<Platforms>
		<Platform
			Name="Win32"/>
	</Platforms>
	<Configurations>
		<Configuration
			Name="Debug|Win32"
			OutputDirectory=".\..\Lib"
			IntermediateDirectory=".\Debug"
			ConfigurationType="2"
			UseOfMFC="0"
			ATLMinimizesCRunTimeLibraryUsage="FALSE"
			CharacterSet="1">
			<Tool
				Name="VCCLCompilerTool"
				AdditionalOptions="/Zm256 "
				Optimization="0"
				AdditionalIncludeDirectories=".,..\Inc,..\..\Core\Inc,..\..\Engine\Inc,..\..\metoolkit\include"
				PreprocessorDefinitions="UWEB_API=__declspec(dllexport);_WINDOWS;UNICODE;_DEBUG;_WIN32_IE=0x0200;WIN32"
				MinimalRebuild="TRUE"
				BasicRuntimeChecks="2"
				RuntimeLibrary="2"
				StructMemberAlignment="3"
				ForceConformanceInForLoopScope="TRUE"
				UsePrecompiledHeader="2"
				PrecompiledHeaderThrough="UWebPrivate.h"
				PrecompiledHeaderFile=".\Debug/UWeb.pch"
				AssemblerListingLocation=".\Debug/"
				ObjectFile=".\Debug/"
				ProgramDataBaseFileName=".\Debug/"
				WarningLevel="4"
				SuppressStartupBanner="TRUE"
				DebugInformationFormat="4"
				CompileAs="0"/>
			<Tool
				Name="VCCustomBuildTool"
				CommandLine="xcopy /Y $(IntDir)\UWeb.dll ..\..\System
"
				Outputs="..\..\System\UWeb.dll"/>
			<Tool
				Name="VCLinkerTool"
				AdditionalOptions="/MACHINE:I386"
				AdditionalDependencies="..\..\Core\Lib\Core.lib ..\..\Engine\Lib\Engine.lib"
				OutputFile="$(IntDir)\UWeb.dll"
				LinkIncremental="2"
				SuppressStartupBanner="TRUE"
				IgnoreDefaultLibraryNames="LIBCD;LIBCMTD;MSVCRTD;LIBC;LIBCMT"
				GenerateDebugInformation="TRUE"
				ProgramDatabaseFile=".\..\Lib/UWeb.pdb"
				SubSystem="2"
				BaseAddress="0x10700000"
				ImportLibrary=".\..\Lib/UWeb.lib"/>
			<Tool
				Name="VCMIDLTool"
				PreprocessorDefinitions="NDEBUG"
				MkTypLibCompatible="TRUE"
				SuppressStartupBanner="TRUE"
				TargetEnvironment="1"
				TypeLibraryName=".\..\Lib/UWeb.tlb"/>
			<Tool
				Name="VCPostBuildEventTool"/>
			<Tool
				Name="VCPreBuildEventTool"/>
			<Tool
				Name="VCPreLinkEventTool"/>
			<Tool
				Name="VCResourceCompilerTool"
				PreprocessorDefinitions="NDEBUG"
				Culture="1033"/>
			<Tool
				Name="VCWebServiceProxyGeneratorTool"/>
			<Tool
				Name="VCWebDeploymentTool"/>
		</Configuration>
		<Configuration
			Name="Release|Win32"
			OutputDirectory=".\..\Lib"
			IntermediateDirectory=".\Release"
			ConfigurationType="2"
			UseOfMFC="0"
			ATLMinimizesCRunTimeLibraryUsage="FALSE"
			CharacterSet="1">
			<Tool
				Name="VCCLCompilerTool"
				AdditionalOptions="/Zm256 "
				Optimization="3"
				GlobalOptimizations="TRUE"
				InlineFunctionExpansion="2"
				EnableIntrinsicFunctions="TRUE"
				FavorSizeOrSpeed="1"
				OmitFramePointers="FALSE"
				OptimizeForProcessor="2"
				AdditionalIncludeDirectories=".,..\Inc,..\..\Core\Inc,..\..\Engine\Inc,..\..\metoolkit\include"
				PreprocessorDefinitions="UWEB_API=__declspec(dllexport);_WINDOWS;NDEBUG;UNICODE;_WIN32_IE=0x0200;WIN32"
				StringPooling="TRUE"
				MinimalRebuild="FALSE"
				RuntimeLibrary="2"
				StructMemberAlignment="3"
				BufferSecurityCheck="FALSE"
				EnableFunctionLevelLinking="TRUE"
				ForceConformanceInForLoopScope="TRUE"
				UsePrecompiledHeader="2"
				PrecompiledHeaderThrough="UWebPrivate.h"
				PrecompiledHeaderFile=".\Release/UWeb.pch"
				AssemblerListingLocation=".\Release/"
				ObjectFile=".\Release/"
				ProgramDataBaseFileName=".\Release/"
				BrowseInformation="0"
				WarningLevel="4"
				WarnAsError="TRUE"
				SuppressStartupBanner="TRUE"
				DebugInformationFormat="0"
				CompileAs="0"/>
			<Tool
				Name="VCCustomBuildTool"
				CommandLine="xcopy /Y $(IntDir)\UWeb.dll ..\..\System
"
				Outputs="..\..\System\UWeb.dll"/>
			<Tool
				Name="VCLinkerTool"
				AdditionalOptions="/MACHINE:I386"
				AdditionalDependencies="..\..\Core\Lib\Core.lib ..\..\Engine\Lib\Engine.lib"
				OutputFile="$(IntDir)\UWeb.dll"
				LinkIncremental="2"
				SuppressStartupBanner="TRUE"
				IgnoreDefaultLibraryNames="LIBCD;LIBCMTD;MSVCRTD;LIBC;LIBCMT"
				ProgramDatabaseFile=".\..\Lib/UWeb.pdb"
				SubSystem="2"
				OptimizeReferences="0"
				BaseAddress="0x10700000"
				ImportLibrary=".\..\Lib/UWeb.lib"/>
			<Tool
				Name="VCMIDLTool"
				PreprocessorDefinitions="NDEBUG"
				MkTypLibCompatible="TRUE"
				SuppressStartupBanner="TRUE"
				TargetEnvironment="1"
				TypeLibraryName=".\..\Lib/UWeb.tlb"/>
			<Tool
				Name="VCPostBuildEventTool"/>
			<Tool
				Name="VCPreBuildEventTool"/>
			<Tool
				Name="VCPreLinkEventTool"/>
			<Tool
				Name="VCResourceCompilerTool"
				PreprocessorDefinitions="NDEBUG"
				Culture="1033"/>
			<Tool
				Name="VCWebServiceProxyGeneratorTool"/>
			<Tool
				Name="VCWebDeploymentTool"/>
		</Configuration>
	</Configurations>
	<Files>
		<Filter
			Name="Src"
			Filter="*.cpp">
			<File
				RelativePath=".\UWeb.cpp">
			</File>
			<File
				RelativePath=".\WebServer.cpp">
			</File>
		</Filter>
		<Filter
			Name="Inc"
			Filter="*.h">
			<File
				RelativePath="..\Inc\UWeb.h">
			</File>
		</Filter>
		<Filter
			Name="Classes"
			Filter="*.uc">
			<File
				RelativePath="..\Classes\HelloWeb.uc">
			</File>
			<File
				RelativePath="..\Classes\ImageServer.uc">
			</File>
			<File
				RelativePath="..\Classes\UWeb.upkg">
			</File>
			<File
				RelativePath="..\Classes\WebApplication.uc">
			</File>
			<File
				RelativePath="..\Classes\WebConnection.uc">
			</File>
			<File
				RelativePath="..\Classes\WebRequest.uc">
			</File>
			<File
				RelativePath="..\Classes\WebResponse.uc">
			</File>
			<File
				RelativePath="..\Classes\WebServer.uc">
			</File>
		</Filter>
	</Files>
	<Globals>
	</Globals>
</VisualStudioProject>
